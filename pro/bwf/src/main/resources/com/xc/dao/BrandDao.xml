<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xc.dao.BrandDao">
    <select id="findBrand" resultType="brand">
        select * from brand
    </select>
    <select id="findBrandById" resultType="brand">
        select * from brand where brand_id=#{brand_id}
    </select>
</mapper>